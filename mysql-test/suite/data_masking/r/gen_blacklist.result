INSTALL PLUGIN data_masking SONAME 'data_masking.so';
SELECT gen_dictionary_load('../../std_data/data_masking/de_cities.txt', 'de_cities');
gen_dictionary_load('../../std_data/data_masking/de_cities.txt', 'de_cities')
