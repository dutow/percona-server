#
# Component Masking Functions: gen_rnd_email()
#
INSTALL COMPONENT 'file://component_masking_functions';
SELECT mask_inner(gen_rnd_email(), 0, 12);
mask_inner(gen_rnd_email(), 0, 12)
XXXXXXXXXXXXX@example.com
SELECT mask_inner(gen_rnd_email(16), 0, 12);
mask_inner(gen_rnd_email(16), 0, 12)
XXXXXXXXXXXXXXXXXXXXXXXX@example.com
SELECT mask_inner(gen_rnd_email(16, 20, "foo.com"), 0, 8);
mask_inner(gen_rnd_email(16, 20, "foo.com"), 0, 8)
XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX@foo.com
SELECT CHARSET(gen_rnd_email());
CHARSET(gen_rnd_email())
utf8mb4
SELECT CHARSET(gen_rnd_email(16));
CHARSET(gen_rnd_email(16))
utf8mb4
SELECT CHARSET(gen_rnd_email(16, 4, "foo.com"));
CHARSET(gen_rnd_email(16, 4, "foo.com"))
utf8mb4
SELECT CHARSET(gen_rnd_email(16, 8, _latin2"foo.com"));
CHARSET(gen_rnd_email(16, 8, _latin2"foo.com"))
latin2
UNINSTALL COMPONENT 'file://component_masking_functions';
